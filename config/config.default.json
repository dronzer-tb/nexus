{
  "server": {
    "port": 8080,
    "host": "0.0.0.0",
    "jwtSecret": "",
    "corsOrigins": []
  },
  "node": {
    "serverUrl": "http://localhost:8080",
    "reportInterval": 5000,
    "hostname": ""
  },
  "database": {
    "path": "./data/nexus.db"
  },
  "nginx": {
    "enabled": false,
    "domain": "",
    "ssl": false,
    "sslType": "none"
  },
  "security": {
    "enableHttps": false,
    "certPath": "",
    "keyPath": ""
  },
  "console": {
    "enabled": true,
    "allowSudo": false,
    "blockedCommands": [
      "rm -rf /",
      "rm -rf /*",
      "mkfs.",
      "dd if=",
      ":(){:|:&};:",
      "chmod -R 777 /",
      "chown -R",
      "> /dev/sda",
      "> /dev/nvme"
    ],
    "blockedPaths": [
      "/etc/passwd",
      "/etc/shadow",
      "/etc/sudoers",
      "/boot",
      "/proc",
      "/sys"
    ]
  },
  "logging": {
    "level": "info",
    "file": "./data/nexus.log"
  }
}
